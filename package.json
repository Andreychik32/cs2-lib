{
    "name": "@ianlucas/cslib",
    "version": "11.4.0",
    "description": "A TypeScript library for manipulating Counter-Strike-related data",
    "license": "MIT",
    "author": "Ian Lucas",
    "repository": "ianlucas/cslib",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "format": "prettier . --write",
        "prepack": "([ -d dist ] && rm -rf dist); tsc",
        "test": "jest",
        "cases-scraper": "tsx scripts/case-scraper.ts",
        "items-generator": "tsx scripts/items-generator.ts > generate-log.txt",
        "non-legacy-paints-scraper": "tsx scripts/non-legacy-paints-scraper.ts",
        "tint-graffiti-images-scraper": "tsx scripts/tint-graffiti-images-scraper.ts",
        "tint-graffiti-names-generator": "tsx scripts/tint-graffiti-names-generator.ts",
        "upgrade": "npx npm-check-updates@latest --reject \"@types/node isbot\" --target minor -u"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^18.15.11",
        "@types/node-fetch": "^2.6.11",
        "cheerio": "^1.0.0-rc.12",
        "dotenv": "^16.4.5",
        "html-entities": "^2.5.2",
        "jest": "^29.7.0",
        "node-fetch": "^2.7.0",
        "prettier": "3.2.5",
        "ts-jest": "^29.1.2",
        "tsx": "^4.7.1",
        "typescript": "^5.4.2"
    }
}
